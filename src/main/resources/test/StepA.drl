package test;

import test.FirstHuman;
import test.SecondHuman;

rule "Step A"
	dialect "mvel"
	when
	    e : FirstHuman()
	    not SecondHuman()
	then
		SecondHuman hu = new SecondHuman();
	   	hu.setName("B");
